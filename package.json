{
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:frontend": "vite build --config vite.config.frontend.mjs",
        "watch:frontend": "vite build --config vite.config.frontend.mjs --watch",
        "build:cms": "vite build --config vite.config.cms.mjs",
        "build:plugins": "for /d %i in (plugins\\*) do if exist %i\\vite.config.mjs vite build --config %i\\vite.config.mjs",
        "build:plugin": "vite build --config plugins\\%npm_config_plugin%\\vite.config.mjs"
    },
    "devDependencies": {
        "browser-sync": "^2.27.10",
        "browser-sync-webpack-plugin": "^2.3.0",
        "laravel-mix": "^6.0.49",
        "laravel-vite-plugin": "^1.0.1",
        "lodash": "^4.17.19",
        "postcss": "^8.4.13",
        "vite": "^5.0.11"
    },
    "dependencies": {
        "@formio/js": "^5.0.0-rc.39",
        "formiojs": "^4.18.2",
        "jquery": "^3.7.1",
        "lazysizes": "^5.3.2",
        "lightgallery": "^2.7.2",
        "owl.carousel": "^2.3.4",
        "vite-plugin-cp": "^4.0.8"
    }
